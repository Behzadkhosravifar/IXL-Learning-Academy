    <div class="dt-sc-hr"></div>

    <!--RecentBlogs fullwidth-background starts-->
    @Html.Partial("IndexPartials/_RecentBlogs")
    <!--RecentBlogs fullwidth-background ends-->

    <div class="dt-sc-hr"></div>

    <!--OurStaffs starts-->
    @Html.Partial("IndexPartials/_OurStaffs")
    <!--OurStaffs ends-->

    <div class="dt-sc-hr"></div>

    <!--Recent Products fullwidth-background starts-->
    @Html.Partial("IndexPartials/_RecentProducts")
    <!--Recent Products fullwidth-background ends-->
</section>
<!--primary ends-->